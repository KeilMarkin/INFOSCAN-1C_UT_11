Процедура ЗаписатьЛог(Barcode, Length = 0, Height = 0, Width = 0, Weight = 0, Событие) Экспорт   
	
	РС = РегистрыСведений.isc_ЛогСобытий.СоздатьМенеджерЗаписи();
	РС.Дата 		= ТекущаяДата();
	РС.Событие 		= Событие;
	РС.Штрихкод 	= Barcode;
	РС.Пользователь = Пользователи.ТекущийПользователь();
	РС.Длина 		= Length;
	РС.Ширина 		= Width;
	РС.Высота 		= Height;
	РС.Масса 		= Weight;
	РС.Записать();  
	
КонецПроцедуры